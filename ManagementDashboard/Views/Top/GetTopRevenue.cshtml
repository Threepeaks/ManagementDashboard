
@model List<ManagementDashboard.Models.GetTopRevenue>

<table class="table table-striped table-sm">
    <thead>
        <tr>
            <th>Com Ref</th>
            <th>Company</th>
            <th class="text-right">Total</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                
                <td>@item.comRef</td>
                <td>@item.Company</td>
                <td class="text-right">@item.total.ToString("N")</td>
            </tr>
        }
    </tbody>
</table>

