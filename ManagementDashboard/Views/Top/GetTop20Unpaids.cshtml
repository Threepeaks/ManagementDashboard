
@model List<ManagementDashboard.Models.GetTop20Unpaids>

<table class="table table-striped table-sm">
    <thead>
        <tr>
            <th>Customer</th>
            <th>Unpaids</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Customer</td>
                <td>@item.Unpaids</td>
                
            </tr>
        }
    </tbody>
</table>
