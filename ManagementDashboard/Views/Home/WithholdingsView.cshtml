
@{
    ViewBag.Title = "WithholdingsView";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="card card-primary">
    <div class="card-header">
        <a href="#" id="NoRetentionDeposit"></a>
        No Deposits & Retentions
    </div>

    <div class="row">

        <div class="col-sm-12">
            <div class="card">
                <div class="card-body">
                    <div class="panel-body1" style="overflow:auto;">
                        <div id="NoRetentionDeposit0">Loading....</div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

@section scripts
{

    <script>
        $().ready(function () {

            $("#PrintThis").click(function () {
                $('#Report').printThis();
            });

        });

    </script>


    <script>

        $().ready(function () {


            function aJaxPost(action, myData, target) {
                $.ajax({
                    method: "POST",
                    url: action,
                    data: myData,
                    success: function (response) {
                        $(target).html(response);
                        // alert(response);
                        // alert(target + " Details saved successfully!!!");
                    },
                    error: function (xhr, ajaxOptions, thrownError) {
                        $(target).html(xhr.status);
                        //alert(xhr.status);
                        //alert(thrownError);
                    }
                });
            }

               //Html.Action("NoRetentionDeposit", "Top", new { id = 0 })
               aJaxPost("@Url.Action("NoRetentionDeposit", "Top")", { id: 0, }, "#NoRetentionDeposit0");






        });

    </script>


}
